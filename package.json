{
  "name": "hezagonal-ts",
  "version": "1.0.0",
  "description": "Esse projeto tem como objetivo servir como base de estrutura hezagonal(port/adapters) com um adicional de CQRS para desacoplar o processo de leitura e escrita do processo de persitÃªncia.",
  "main": "index.js",
  "directories": {
    "lib": "src",
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "prestart:node": "npm run build",
    "start:node": "node ./dist/index.js",
    "prestart:watch": "pm2 delete pm2.default.json",
    "start:watch": "pm2 start --no-daemon -s pm2.default.json",
    "stop:watch": "pm2 kill",
    "clear": "npm run clear:build && npm run clear:modules",
    "clear:build": "rm -rf ./dist",
    "clear:modules": "rm -rf ./node_modules",
    "prebuild": "rm -rf ./dist && rm -f tsconfig.tsbuildinfo",
    "build": "tsc",
    "test": "npm run test:unit",
    "test:unit": "nyc mocha --opts ./test/unit/mocha.opts",
    "lint": "npm run lint:format && npm run lint:style",
    "lint:fix": "npm run lint:format:fix && npm run lint:style:fix",
    "lint:format": "prettier --check '{src,test}/**/*.ts'",
    "lint:format:fix": "prettier --write '{src,test}/**/*.ts'",
    "lint:style": "tslint --project .",
    "lint:style:fix": "tslint --project . --fix"
  },
  "keywords": [
    "ts",
    "typescript",
    "hezagonal",
    "ddd",
    "base"
  ],
  "author": "Gustavo S. Rodrigues",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.0",
    "@babel/node": "7.5.0",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.5.0",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/preset-env": "7.5.0",
    "@babel/preset-es2017": "7.0.0-beta.53",
    "@babel/preset-typescript": "7.3.3",
    "@babel/register": "7.4.4",
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@types/chai": "4.2.18",
    "@types/chai-as-promised": "7.1.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.0.12",
    "@types/sinon": "7.0.13",
    "@typescript-eslint/parser": "1.11.0",
    "babel-register": "6.26.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "husky": "3.0.0",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "pm2": "3.5.1",
    "prettier": "1.18.2",
    "sinon": "7.3.2",
    "source-map-support": "0.5.12",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-airbnb-base": "0.3.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "@types/dotenv": "6.1.1",
    "@types/verror": "^1.10.3",
    "dotenv": "8.0.0",
    "inversify": "5.0.1",
    "verror": "^1.10.0"
  }
}
